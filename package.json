{
  "name": "brainstorm-mockups",
  "version": "1.0.0",
  "description": "University of Chicago Library Design System Documentation",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:eleventy": "eleventy --serve",
    "dev:sass": "sass --watch src/styles:dist/styles",
    "build": "npm-run-all build:*",
    "build:eleventy": "eleventy",
    "build:sass": "sass src/styles:dist/styles --style=compressed",
    "prod": "npm run build"
  },
  "keywords": [
    "design-system",
    "bootstrap",
    "eleventy",
    "static-site"
  ],
  "author": "University of Chicago Library",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "bootstrap": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5"
  }
}
